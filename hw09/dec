def intersection(A, B):
    return [x for x in A if x in A and x in B]

A = [1, 2, 3]
B = [2, 3, 4]
intersection(A, B)
    


import time
def time_logging(f):
    def inner( *args ):
        start_time = time.time()
        run = f(*args)
        time = time.time() - start_time
        return "execution time:" + time + run
    return inner
    
